<h1>Add New Property</h1>
<%= form_for @user  do |f| %>
    <%= f.label :address %><br>
    <%= f.text_field :address %><br>
    <%= f.label :price %><br>
    <%= f.text_field :price %><br>
    <%= f.label :description %><br>
    <%= f.text_field :description %><br>
    <%= f.label :image_url %><br>
    <%= f.text_field :image_url %><br>
    <%= f.label :current_tenant %><br>
    <%= f.text_field :bio %><br>
   
    
    <br />
  
    <%= f.submit %>
  <% end %>