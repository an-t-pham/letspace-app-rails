<%= form_for @property, url: @url  do |f| %>
    <%= f.label :address %><br>
    <%= f.text_field :address %><br>
    <%= f.label :price %><br>
    <%= f.text_field :price %><br>
    <%= f.label :description %><br>
    <%= f.text_area :description %><br>
    <%= f.label :image_url %><br>
    <%= f.text_field :image_url %><br>
    <%= f.label :tenant, "Current Tenant" %><br>
    <%= f.collection_select  :tenant_id, @available_tenants, :id, :name, :include_blank => "No Tenant" %><br>
    
    <br />
  
    <%= f.submit %>
  <% end %>