<h1>Add New Review</h1>
<%= form_for @review do |f| %>
    <%= f.label :rating %><br>
    <%= f.text_field :rating %><br>
    <%= f.label :title %><br>
    <%= f.text_field :title %><br>
    <%= f.label :content %><br>
    <%= f.text_area :content %><br>

    <br />
    <% f.hidden_field :property_id

    <%= f.submit "Sign In" %>
  <% end %>